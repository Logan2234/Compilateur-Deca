class A {
    protected int a;
    float b = 0.0;

    int getA(){
        return a;
    }

    void setA(int a){
        this.a = a;
    }
}

class B extends A {
    void init(int a, float b){
        setA(a);
        this.b = b;
    }

    boolean equals(Object o){
        if (o instanceof B){
            if ((B)(o).b == b && ((B)(o).getA() == getA())){
                return true;
            }
        }
        return false;
    }
}

